@prefix cont: <http://www.semanticweb.org/rpg/ontologies/2021/5/container-ontology#> .
@prefix om: <http://www.ontology-of-units-of-measure.org/resource/om-2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

om:abbreviation a owl:AnnotationProperty .

<http://www.semanticweb.org/rpg/ontologies/2021/5/container-ontology> a owl:Ontology ;
    rdfs:label "Container Ontology"@en ;
    om:abbreviation "cont:" ;
    owl:imports <http://sbols.org/v3>,
        <http://www.semanticweb.org/rpg/ontologies/2021/5/om-subset> ;
    owl:versionIRI <http://www.semanticweb.org/rpg/ontologies/2021/5/container-ontology/v1> .

cont:Centrifuge a owl:Class,
        owl:NamedIndividual ;
    rdfs:comment "Likely this should actually be imported from another ontology of lab equipment, if one is available." ;
    rdfs:subClassOf cont:LabMachinery .

cont:PlateReader a owl:Class,
        owl:NamedIndividual ;
    rdfs:comment "Likely this should actually be imported from another ontology of lab equipment, if one is available." ;
    rdfs:subClassOf cont:LabMachinery .

cont:SLAS-1-2004 a owl:Class ;
    rdfs:comment "SLAS 1-2004 specifies the microplate footprint standard"^^xsd:string ;
    rdfs:subClassOf cont:Plate ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( [ a owl:Restriction ;
                        owl:onProperty cont:cornerRadius ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                            owl:hasValue om:millimetre ;
                                            owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                            owl:allValuesFrom [ a rdfs:Datatype ;
                                                    owl:onDatatype xsd:float ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "1.58"^^xsd:float ] [ xsd:maxInclusive "4.78"^^xsd:float ] ) ] ;
                                            owl:onProperty om:hasNumericalValue ] ) ] ] [ a owl:Restriction ;
                        owl:onProperty cont:length ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                            owl:hasValue om:millimetre ;
                                            owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                            owl:allValuesFrom [ a rdfs:Datatype ;
                                                    owl:onDatatype xsd:float ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "127.51"^^xsd:float ] [ xsd:maxInclusive "128.01"^^xsd:float ] ) ] ;
                                            owl:onProperty om:hasNumericalValue ] ) ] ] [ a owl:Restriction ;
                        owl:onProperty cont:width ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                            owl:hasValue om:millimetre ;
                                            owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                            owl:allValuesFrom [ a rdfs:Datatype ;
                                                    owl:onDatatype xsd:float ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "85.23"^^xsd:float ] [ xsd:maxInclusive "85.73"^^xsd:float ] ) ] ;
                                            owl:onProperty om:hasNumericalValue ] ) ] ] ) ] .

cont:SLAS-2-2004-4-1 a owl:Class ;
    rdfs:label "SLAS 2-2004 Typical Height with Clearance"^^xsd:string ;
    rdfs:comment "Specification of Microplates meeting the specification of ANSI SLAS-2-2004, R2012, based on section 4.1, \"Typical Height with Clearance.\""^^xsd:string ;
    rdfs:subClassOf cont:Plate ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( cont:SLAS-2-2004-4-2 [ a owl:Restriction ;
                        owl:onProperty cont:clearanceToPlateBottom ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                            owl:hasValue om:millimetre ;
                                            owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                            owl:allValuesFrom [ a rdfs:Datatype ;
                                                    owl:onDatatype xsd:float ;
                                                    owl:withRestrictions ( [ xsd:maxInclusive "1.0"^^xsd:float ] ) ] ;
                                            owl:onProperty om:hasNumericalValue ] ) ] ] ) ] .

cont:TFEPWD a om:Measure,
        owl:NamedIndividual ;
    om:hasNumericalValue "20.1"^^xsd:float ;
    om:hasUnit om:millimetre ;
    cont:measureType om:Depth ;
    rdfs:comment "The measure corresponding to the well height of the ThermoFisher EnduraPlate 96 well" .

<http://www.semanticweb.org/rpg/ontologies/2021/5/container-ontology#ThermoFisher/EnduraPlate_96Well> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty cont:cornerRadius ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                owl:hasValue om:millimetre ;
                                owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                owl:hasValue 4.0 ;
                                owl:onProperty om:hasNumericalValue ] ) ] ],
        [ a owl:Restriction ;
            owl:onProperty cont:length ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                owl:hasValue om:millimetre ;
                                owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                owl:onProperty om:hasNumericalValue ;
                                owl:someValuesFrom [ a rdfs:Datatype ;
                                        owl:onDatatype xsd:float ;
                                        owl:withRestrictions ( [ xsd:minInclusive "127.26"^^xsd:float ] [ xsd:maxInclusive "128.26"^^xsd:float ] ) ] ] ) ] ],
        [ a owl:Restriction ;
            owl:onProperty cont:plateHeight ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                owl:hasValue om:millimetre ;
                                owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                owl:hasValue 7.50 ;
                                owl:onProperty om:hasNumericalValue ] ) ] ],
        [ a owl:Restriction ;
            owl:onProperty cont:wellDepth ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                owl:hasValue om:millimetre ;
                                owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                owl:hasValue om:Depth ;
                                owl:onProperty cont:measureType ] [ a owl:Restriction ;
                                owl:hasValue "20.1"^^xsd:float ;
                                owl:onProperty om:hasNumericalValue ] ) ] ],
        [ a owl:Restriction ;
            owl:onProperty cont:width ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                owl:hasValue om:millimetre ;
                                owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                owl:onProperty om:hasNumericalValue ;
                                owl:someValuesFrom [ a rdfs:Datatype ;
                                        owl:onDatatype xsd:float ;
                                        owl:withRestrictions ( [ xsd:minInclusive "84.98"^^xsd:float ] [ xsd:maxInclusive "85.98"^^xsd:float ] ) ] ] ) ] ],
        [ a owl:Restriction ;
            owl:hasValue cont:ThermoFisher ;
            owl:onProperty cont:hasVendorFirm ],
        [ a owl:Restriction ;
            owl:onClass cont:VendorFirm ;
            owl:onProperty cont:hasVendorFirm ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:hasValue 96 ;
            owl:onProperty cont:wellCount ],
        cont:Plate,
        cont:SLAS-4-2004 .

cont:VWRCentrifugePCRPlateSpec a owl:Class ;
    owl:equivalentClass [ a owl:Restriction ;
            owl:onProperty cont:wellDepth ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                owl:hasValue om:millimetre ;
                                owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                owl:hasValue om:Depth ;
                                owl:onProperty cont:measureType ] [ a owl:Restriction ;
                                owl:hasValue "20.1"^^xsd:float ;
                                owl:onProperty om:hasNumericalValue ] ) ] ] .

cont:catalogNumber a owl:DatatypeProperty ;
    rdfs:label "catalogNumber" ;
    rdfs:domain cont:CatalogEntry ;
    rdfs:range xsd:string ;
    rdfs:subPropertyOf owl:topDataProperty .

cont:centrifugeResistance a owl:ObjectProperty ;
    rdfs:label "centrifugeResistance" ;
    rdfs:comment "What is the maximum force the container can (or must) resist (for the assay/protocol if this is defined there)."^^xsd:string ;
    rdfs:domain cont:Container ;
    rdfs:range [ a owl:Class ;
            owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                        owl:allValuesFrom om:Force ;
                        owl:onProperty cont:measureType ] ) ] .

cont:coating a owl:ObjectProperty ;
    rdfs:label "coating" ;
    rdfs:domain cont:Container ;
    rdfs:range cont:CoatingMaterial ;
    rdfs:subPropertyOf owl:topObjectProperty .

cont:columnCount a owl:DatatypeProperty ;
    rdfs:label "columnCount" ;
    rdfs:domain cont:Plate ;
    rdfs:range xsd:positiveInteger ;
    rdfs:subPropertyOf cont:containerDataProperty .

cont:compatibleCentrifuges a owl:ObjectProperty ;
    rdfs:label "compatibleCentrifuges" ;
    rdfs:comment "Centrifuge TYPES with which this container is compatible."^^xsd:string ;
    rdfs:domain cont:Container ;
    rdfs:subPropertyOf cont:compatibleLabEquipment .

cont:compatiblePlateReaders a owl:ObjectProperty ;
    rdfs:label "compatiblePlateReaders" ;
    rdfs:subPropertyOf cont:compatibleLabEquipment .

cont:hasCatalogEntry a owl:ObjectProperty ;
    rdfs:label "hasCatalogEntry" ;
    rdfs:domain cont:LabEquipment ;
    rdfs:range cont:CatalogEntry ;
    rdfs:subPropertyOf owl:topObjectProperty .

cont:isOpticallyTransparent a owl:DatatypeProperty ;
    rdfs:label "isOpticallyTransparent" ;
    rdfs:domain cont:Container ;
    rdfs:range xsd:boolean ;
    rdfs:subPropertyOf cont:containerDataProperty .

cont:isStackable a owl:DatatypeProperty ;
    rdfs:range xsd:boolean ;
    rdfs:subPropertyOf cont:containerDataProperty .

cont:lengthAtEdge a owl:ObjectProperty ;
    rdfs:label "lengthAtEdge" ;
    rdfs:comment """ANSI SLAS spec 1.2004 (plate footprint spec) specifies different tolerances for the length within 12.7mm of the edge and in general, the former being tighter than the latter.

This property is for the length within 12.7 mm of the edge; the other property is lengthOverall.""" ;
    rdfs:subPropertyOf cont:length ;
    owl:propertyDisjointWith cont:lengthOverall .

cont:rowCount a owl:DatatypeProperty ;
    rdfs:domain cont:Plate ;
    rdfs:range xsd:positiveInteger ;
    rdfs:subPropertyOf cont:containerDataProperty .

cont:vendorName a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( cont:LabEquipment cont:VendorFirm ) ] ;
    rdfs:range xsd:string .

om:Force a owl:Class .

om:hasQuantity a owl:ObjectProperty .

cont:CoatingMaterial a owl:Class .

cont:SLAS-2-2004-4-2 a owl:Class ;
    rdfs:label "SLAS 2-2004 Typical Height with Clearance"^^xsd:string ;
    rdfs:comment "Specification of Microplates meeting the specification of ANSI SLAS-2-2004, R2012, based on section 4.2, \"Typical Height.\""^^xsd:string ;
    rdfs:subClassOf cont:Plate ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( cont:Plate [ a owl:Class ;
                        owl:unionOf ( [ a owl:Restriction ;
                                    owl:hasValue 1536 ;
                                    owl:onProperty cont:wellCount ] [ a owl:Restriction ;
                                    owl:hasValue 384 ;
                                    owl:onProperty cont:wellCount ] [ a owl:Restriction ;
                                    owl:hasValue 96 ;
                                    owl:onProperty cont:wellCount ] ) ] [ a owl:Restriction ;
                        owl:onProperty cont:overallPlateHeight ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                            owl:hasValue om:millimetre ;
                                            owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                            owl:allValuesFrom [ a rdfs:Datatype ;
                                                    owl:onDatatype xsd:float ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "13.59"^^xsd:float ] [ xsd:maxInclusive "15.11"^^xsd:float ] ) ] ;
                                            owl:onProperty om:hasNumericalValue ] ) ] ] [ a owl:Restriction ;
                        owl:onProperty cont:plateHeight ;
                        owl:someValuesFrom [ a owl:Class ;
                                owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                                            owl:hasValue om:millimetre ;
                                            owl:onProperty om:hasUnit ] [ a owl:Restriction ;
                                            owl:allValuesFrom [ a rdfs:Datatype ;
                                                    owl:onDatatype xsd:float ;
                                                    owl:withRestrictions ( [ xsd:minInclusive "14.1"^^xsd:float ] [ xsd:maxInclusive "14.6"^^xsd:float ] ) ] ;
                                            owl:onProperty om:hasNumericalValue ] ) ] ] ) ] .

cont:SLAS-4-2004 a owl:Class ;
    rdfs:label "SLAS 4-2004 Well Placement standard"^^xsd:string ;
    rdfs:comment "We do not model the well placement in detail, simply specifying whether or not it complies with 4-2004."^^xsd:string ;
    rdfs:subClassOf cont:Plate ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( cont:Plate [ a owl:Class ;
                        owl:unionOf ( [ a owl:Restriction ;
                                    owl:hasValue 1586 ;
                                    owl:onProperty cont:wellCount ] [ a owl:Restriction ;
                                    owl:hasValue 384 ;
                                    owl:onProperty cont:wellCount ] [ a owl:Restriction ;
                                    owl:hasValue 96 ;
                                    owl:onProperty cont:wellCount ] ) ] ) ],
        [ a owl:Restriction ;
            owl:hasValue true ;
            owl:onProperty cont:isSLAS1-2004compliant ] .

cont:ThermoFisher a cont:VendorFirm,
        owl:NamedIndividual ;
    cont:vendorName "ThermoFisher"^^xsd:string .

cont:Well a owl:Class ;
    rdfs:subClassOf cont:Container .

cont:isSLAS1-2004compliant a owl:DatatypeProperty ;
    rdfs:label "isSLAS1-2004compliant" ;
    rdfs:range xsd:boolean ;
    rdfs:subPropertyOf cont:containerDataProperty .

cont:lengthOverall a owl:ObjectProperty ;
    rdfs:label "lengthOverall" ;
    rdfs:comment """ANSI SLAS spec 1.2004 (plate footprint spec) specifies different tolerances for the length within 12.7mm of the edge and in general, the former being tighter than the latter.

This property is for the length anywhere on the plate (the more permissive constraint); the other property is lengthAtEdge."""@en ;
    rdfs:subPropertyOf cont:length .

cont:ContainerRoot a owl:Class ;
    rdfs:label "ContainerRoot" ;
    rdfs:comment "This is the root class for the classes defined in the Container Ontology. It is introduced to be the bridge between the container ontology and the SBOL Ontology. In turn, this connects to the sbol:Identified class, in order to make it possible for the container ontology classes to be translated into Python classes by the SBOL factory." ;
    rdfs:subClassOf <http://sbols.org/v3#Identified> .

cont:LabMachinery a owl:Class ;
    rdfs:comment "This is likely a poor name, but it is intended to refer to items of equipment that act upon containers and/or their contents." ;
    rdfs:subClassOf cont:LabEquipment .

cont:clearanceToPlateBottom a owl:ObjectProperty ;
    rdfs:label "clearanceToPlateBottom" ;
    rdfs:comment "The minimum clearance from the resting plane to the plane of the bottom external surface of the wells."@en ;
    rdfs:subPropertyOf cont:height .

cont:compatibleLabEquipment a owl:ObjectProperty ;
    rdfs:label "compatibleLabEquipment" .

cont:diameter a owl:ObjectProperty ;
    rdfs:label "diameter" ;
    rdfs:range [ a owl:Restriction ;
            owl:allValuesFrom om:Diameter ;
            owl:onProperty om:hasQuantity ],
        om:Measure ;
    rdfs:subPropertyOf cont:geometricProperty .

cont:hasVendorFirm a owl:ObjectProperty ;
    rdfs:label "hasVendorFirm" ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( cont:CatalogEntry cont:LabEquipment ) ] ;
    rdfs:range cont:VendorFirm ;
    rdfs:subPropertyOf owl:topObjectProperty .

cont:overallPlateHeight a owl:ObjectProperty ;
    rdfs:label "overallPlateHeight" ;
    rdfs:comment "The overall plate height, measured from ... the resting plane ... to the maximum protrusion of the plate."@en ;
    rdfs:subPropertyOf cont:height ;
    owl:propertyDisjointWith cont:plateHeight .

cont:cornerRadius a owl:ObjectProperty ;
    rdfs:label "cornerRadius" ;
    rdfs:subPropertyOf cont:geometricProperty .

cont:volume a owl:ObjectProperty ;
    rdfs:label "volume" ;
    rdfs:subPropertyOf cont:geometricProperty .

cont:wellShape a owl:ObjectProperty ;
    rdfs:label "wellShape" ;
    rdfs:domain cont:Container ;
    rdfs:range [ a owl:Restriction ;
            owl:allValuesFrom om:Height ;
            owl:onProperty cont:measureType ] ;
    rdfs:subPropertyOf cont:geometricProperty .

owl:topObjectProperty rdfs:domain cont:LabEquipment ;
    rdfs:range cont:CatalogEntry .

om:Depth a owl:NamedIndividual .

cont:CatalogEntry a owl:Class ;
    rdfs:subClassOf cont:ContainerRoot .

cont:VendorFirm a owl:Class .

cont:plateHeight a owl:ObjectProperty ;
    rdfs:label "plateHeight" ;
    rdfs:comment "The plate height measured from ... the resting plane... to the maximum protrusion of the perimeter wells."@en ;
    rdfs:domain cont:Plate ;
    rdfs:subPropertyOf cont:height .

cont:width a owl:ObjectProperty ;
    rdfs:label "width" ;
    rdfs:subPropertyOf cont:geometricProperty .

cont:wellDepth a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "wellDepth" ;
    rdfs:domain cont:Container ;
    rdfs:range [ a owl:Class ;
            owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                        owl:hasValue om:Depth ;
                        owl:onProperty cont:measureType ] ) ] ;
    rdfs:subPropertyOf cont:geometricProperty .

cont:LabEquipment a owl:Class ;
    rdfs:subClassOf cont:ContainerRoot .

cont:containerDataProperty a owl:DatatypeProperty ;
    rdfs:label "containerDataProperty" ;
    rdfs:domain cont:Container .

cont:height a owl:ObjectProperty ;
    rdfs:label "height" ;
    rdfs:range [ a owl:Class ;
            owl:intersectionOf ( om:Measure [ a owl:Restriction ;
                        owl:allValuesFrom om:Height ;
                        owl:onProperty cont:measureType ] ) ] ;
    rdfs:subPropertyOf cont:geometricProperty .

cont:length a owl:ObjectProperty ;
    rdfs:label "label" ;
    rdfs:subPropertyOf cont:geometricProperty .

cont:measureType a owl:ObjectProperty ;
    rdfs:label "measureType" ;
    rdfs:domain om:Measure ;
    rdfs:range om:Quantity .

cont:wellCount a owl:DatatypeProperty ;
    rdfs:domain cont:Plate ;
    rdfs:range xsd:positiveInteger ;
    rdfs:subPropertyOf cont:containerDataProperty .

cont:geometricProperty a owl:ObjectProperty ;
    rdfs:label "geometricProperty" ;
    rdfs:domain cont:Container ;
    rdfs:range om:Measure .

cont:Container a owl:Class ;
    rdfs:subClassOf cont:LabEquipment .

cont:Plate a owl:Class ;
    rdfs:subClassOf cont:Container ;
    owl:disjointWith cont:Well .

[] a owl:AllDisjointProperties ;
    owl:members ( cont:clearanceToPlateBottom cont:overallPlateHeight cont:plateHeight ) .

[] a owl:AllDisjointProperties ;
    owl:members ( cont:cornerRadius cont:diameter cont:height cont:length cont:volume cont:wellDepth cont:wellShape cont:width ) .

[] a owl:AllDisjointProperties ;
    owl:members ( cont:diameter cont:height cont:length cont:volume cont:wellDepth cont:wellShape cont:width ) .

[] a owl:AllDisjointProperties ;
    owl:members ( cont:height cont:volume cont:wellDepth cont:wellShape ) .

